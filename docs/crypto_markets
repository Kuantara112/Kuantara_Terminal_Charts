  <script>
    // ==========================
    //  QUOTES TABLE (Dummy)
    // ==========================
    const tableBody = document.querySelector("#indicesTable tbody");
    const searchInput = document.getElementById("searchInput");

    let latestQuotes = [];

    function updateQuotesTable(data) {
      latestQuotes = Array.isArray(data) ? data : [];
      renderQuotesTable();
    }

    function renderQuotesTable() {
      const filter = (searchInput.value || "").trim().toLowerCase();
      tableBody.innerHTML = "";

      latestQuotes
        .filter(row => {
          if (!filter) return true;
          const symbol = (row.symbol || "").toLowerCase();
          const name = (row.name || "").toLowerCase();
          return symbol.includes(filter) || name.includes(filter);
        })
        .forEach(row => {
          const tr = document.createElement("tr");

          const changePct = Number(row.changePct || 0);
          const change = Number(row.change || 0);
          const isUp = changePct > 0;
          const isDown = changePct < 0;

          tr.innerHTML = `
            <td>${row.symbol || ""}</td>
            <td>${row.name || ""}</td>
            <td>${formatNumber(row.last)}</td>
            <td class="${isUp ? "text-up" : isDown ? "text-down" : ""}">
              ${formatSigned(change)}
            </td>
            <td class="${isUp ? "text-up" : isDown ? "text-down" : ""}">
              ${formatSigned(changePct)}%
            </td>
            <td>${formatNumber(row.dailyHigh)}</td>
            <td>${formatNumber(row.dailyLow)}</td>
            <td>${formatNumber(row.weeklyHigh)}</td>
            <td>${formatNumber(row.weeklyLow)}</td>
            <td>${row.session || ""}</td>
          `;

          tableBody.appendChild(tr);
        });
    }

    function formatNumber(value) {
      const num = Number(value);
      if (Number.isNaN(num)) return "";
      const abs = Math.abs(num);

      if (abs >= 1000) {
        return num.toLocaleString(undefined, { maximumFractionDigits: 2 });
      }
      if (abs >= 1) {
        return num.toFixed(2);
      }
      if (abs >= 0.01) {
        return num.toFixed(4);
      }
      return num.toFixed(6);
    }

    function formatSigned(value) {
      const num = Number(value);
      if (Number.isNaN(num) || num === 0) return formatNumber(0);
      const sign = num > 0 ? "+" : "";
      const abs = Math.abs(num);

      let formatted;
      if (abs >= 1000) {
        formatted = abs.toLocaleString(undefined, { maximumFractionDigits: 2 });
      } else if (abs >= 1) {
        formatted = abs.toFixed(2);
      } else if (abs >= 0.01) {
        formatted = abs.toFixed(4);
      } else {
        formatted = abs.toFixed(6);
      }
      return sign + formatted;
    }

    searchInput.addEventListener("input", renderQuotesTable);

    // ==========================
    //  CAPITAL FLOW (Dummy)
    // ==========================
    const capitalFlowCanvas = document.getElementById("capitalFlowCanvas");
    const capitalFlowCtx = capitalFlowCanvas.getContext("2d");
    let capitalFlowData = [];

    function updateCapitalFlow(data) {
      capitalFlowData = (Array.isArray(data) ? data : []).slice();
      capitalFlowData.sort((a, b) => (b.changePct || 0) - (a.changePct || 0));
      drawCapitalFlow();
    }

    function resizeCanvasToContainer(canvas) {
      const parent = canvas.parentElement;
      const rect = parent.getBoundingClientRect();
      const dpr = window.devicePixelRatio || 1;

      canvas.width = rect.width * dpr;
      canvas.height = rect.height * dpr;
      canvas.style.width = rect.width + "px";
      canvas.style.height = rect.height + "px";

      const ctx = canvas.getContext("2d");
      ctx.setTransform(dpr, 0, 0, dpr, 0, 0);
    }

    function drawCapitalFlow() {
      if (!capitalFlowCanvas) return;
      resizeCanvasToContainer(capitalFlowCanvas);
      const ctx = capitalFlowCtx;
      const width = capitalFlowCanvas.clientWidth;
      const height = capitalFlowCanvas.clientHeight;

      ctx.clearRect(0, 0, width, height);

      if (!capitalFlowData.length) {
        ctx.fillStyle = "#555";
        ctx.font = "10px 'Fira Code'";
        ctx.fillText("Waiting for dummy data‚Ä¶", 8, 16);
        return;
      }

      const margin = { top: 8, right: 16, bottom: 20, left: 40 };
      const chartWidth = width - margin.left - margin.right;
      const chartHeight = height - margin.top - margin.bottom;
      const zeroY = margin.top + chartHeight / 2;

      ctx.strokeStyle = "#444";
      ctx.lineWidth = 1;
      ctx.beginPath();
      ctx.moveTo(margin.left, zeroY);
      ctx.lineTo(margin.left + chartWidth, zeroY);
      ctx.stroke();

      const maxAbsChange = capitalFlowData.reduce((acc, d) => {
        const val = Math.abs(d.changePct || 0);
        return val > acc ? val : acc;
      }, 0.01);

      const barWidth = (chartWidth / Math.max(capitalFlowData.length, 1)) * 0.7;
      const xStep = chartWidth / Math.max(capitalFlowData.length, 1);

      ctx.textAlign = "center";
      ctx.font = "9px 'Fira Code'";

      const upColor = "#22c55e";
      const downColor = "#ef4444";

      capitalFlowData.forEach((d, idx) => {
        const xCenter = margin.left + xStep * idx + xStep / 2;
        const value = d.changePct || 0;
        const barHeight = (Math.abs(value) / maxAbsChange) * (chartHeight / 2);

        const isPositive = value >= 0;
        ctx.fillStyle = isPositive ? upColor : downColor;

        const y = isPositive ? zeroY - barHeight : zeroY;
        ctx.fillRect(xCenter - barWidth / 2, y, barWidth, barHeight);

        ctx.fillStyle = "#9b9b9b";
        ctx.save();
        ctx.translate(xCenter, height - 4);
        ctx.rotate(-Math.PI / 4);
        ctx.fillText((d.symbol || "").toUpperCase(), 0, 0);
        ctx.restore();
      });
    }

    // ==========================
    //  RRG (Dummy)
    // ==========================
    const rrgCanvas = document.getElementById("rrgCanvas");
    const rrgCtx = rrgCanvas.getContext("2d");
    let rrgData = [];

    function updateRRG(data) {
      rrgData = Array.isArray(data) ? data : [];
      drawRRG();
    }

    function drawRRG() {
      if (!rrgCanvas) return;
      resizeCanvasToContainer(rrgCanvas);
      const ctx = rrgCtx;
      const width = rrgCanvas.clientWidth;
      const height = rrgCanvas.clientHeight;

      ctx.clearRect(0, 0, width, height);

      const margin = { top: 10, right: 10, bottom: 16, left: 16 };
      const chartWidth = width - margin.left - margin.right;
      const chartHeight = height - margin.top - margin.bottom;

      const centerX = margin.left + chartWidth / 2;
      const centerY = margin.top + chartHeight / 2;

      ctx.strokeStyle = "#444";
      ctx.lineWidth = 1;

      ctx.beginPath();
      ctx.moveTo(centerX, margin.top);
      ctx.lineTo(centerX, margin.top + chartHeight);
      ctx.stroke();

      ctx.beginPath();
      ctx.moveTo(margin.left, centerY);
      ctx.lineTo(margin.left + chartWidth, centerY);
      ctx.stroke();

      ctx.strokeStyle = "#2b2b2b";
      ctx.strokeRect(margin.left, margin.top, chartWidth / 2, chartHeight / 2);
      ctx.strokeRect(centerX, margin.top, chartWidth / 2, chartHeight / 2);
      ctx.strokeRect(margin.left, centerY, chartWidth / 2, chartHeight / 2);
      ctx.strokeRect(centerX, centerY, chartWidth / 2, chartHeight / 2);

      ctx.fillStyle = "#9b9b9b";
      ctx.font = "10px 'Fira Code'";
      ctx.fillText("RS-Ratio ‚Üí", centerX + 4, margin.top + 10);
      ctx.save();
      ctx.translate(margin.left + 4, centerY - 4);
      ctx.rotate(-Math.PI / 2);
      ctx.fillText("RS-Momentum ‚Üë", 0, 0);
      ctx.restore();

      ctx.font = "9px 'Fira Code'";
      ctx.fillText("Leading",   margin.left + 4, margin.top + 12);
      ctx.fillText("Weakening", centerX + 4,    margin.top + 12);
      ctx.fillText("Improving", margin.left + 4, centerY + 12);
      ctx.fillText("Lagging",   centerX + 4,    centerY + 12);

      if (!rrgData.length) {
        ctx.fillStyle = "#555";
        ctx.fillText("Waiting for dummy data‚Ä¶", margin.left + 4, margin.top + 26);
        return;
      }

      const baseRatio = 100;
      const baseMomentum = 100;

      const maxDeltaRatio = rrgData.reduce((acc, d) => {
        const diff = Math.abs((d.rsRatio || baseRatio) - baseRatio);
        return diff > acc ? diff : acc;
      }, 1);

      const maxDeltaMomentum = rrgData.reduce((acc, d) => {
        const diff = Math.abs((d.rsMomentum || baseMomentum) - baseMomentum);
        return diff > acc ? diff : acc;
      }, 1);

      const maxRadiusX = chartWidth / 2 - 10;
      const maxRadiusY = chartHeight / 2 - 10;

      rrgData.forEach(d => {
        const ratio = d.rsRatio || baseRatio;
        const momentum = d.rsMomentum || baseMomentum;

        const dx = ((ratio - baseRatio) / maxDeltaRatio) * maxRadiusX;
        const dy = ((momentum - baseMomentum) / maxDeltaMomentum) * maxRadiusY;

        const x = centerX + dx;
        const y = centerY - dy;

        let color = "#3b82f6";
        if (ratio >= baseRatio && momentum >= baseMomentum) color = "#22c55e";   // Leading
        else if (ratio >= baseRatio && momentum < baseMomentum) color = "#ffbf00"; // Weakening
        else if (ratio < baseRatio && momentum < baseMomentum) color = "#ef4444";  // Lagging

        ctx.fillStyle = color;
        ctx.beginPath();
        ctx.arc(x, y, 4, 0, Math.PI * 2);
        ctx.fill();

        ctx.fillStyle = "#f5f5f5";
        ctx.font = "8px 'Fira Code'";
        ctx.fillText((d.symbol || "").toUpperCase(), x + 5, y - 4);
      });
    }

    // ==========================
    //  DUMMY DATA ‚Äì CRYPTO
    // ==========================
    function loadMockData() {
      const mockQuotes = [
